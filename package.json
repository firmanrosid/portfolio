{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "type-check": "tsc --noEmit",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test:wdio": "wdio run tests/wdio/wdio.conf.ts",
    "test:playwright": "playwright test -c tests/playwright/playwright.config.ts",
    "report:playwright": "playwright show-report tests/playwright/report",
    "report:allure": "allure generate tests/playwright/allure-results --clean -o tests/playwright/allure-report && allure open tests/playwright/allure-report"
  },
  "dependencies": {
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^20.19.28",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@wdio/cli": "^9.23.0",
    "@wdio/local-runner": "^9.23.0",
    "@wdio/mocha-framework": "^9.23.0",
    "@wdio/spec-reporter": "^9.20.0",
    "allure-commandline": "^2.36.0",
    "allure-playwright": "^3.4.4",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "webdriverio": "^9.23.0"
  },
  "ignoreScripts": [
    "sharp",
    "unrs-resolver"
  ],
  "trustedDependencies": [
    "sharp",
    "unrs-resolver"
  ]
}
